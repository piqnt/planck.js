{
  "name": "planck",
  "version": "1.0.0-beta.0",
  "description": "2D JavaScript/TypeScript physics engine for cross-platform HTML5 game development",
  "homepage": "https://github.com/shakiba/planck.js",
  "keywords": [
    "box2d",
    "html5",
    "javascript",
    "typescript",
    "game",
    "physics",
    "engine",
    "2d",
    "mobile"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/shakiba/planck.js.git"
  },
  "main": "dist/planck.js",
  "types": "dist/planck.d.ts",
  "author": "Ali Shakiba",
  "contributors": [
    "Erin Catto",
    "Ali Shakiba",
    "Oliver Zell"
  ],
  "exports": {
    ".": {
      "types": "./dist/planck.d.ts",
      "default": "./dist/planck.js"
    },
    "./with-testbed": {
      "types": "./dist/planck-with-testbed.d.ts",
      "default": "./dist/planck-with-testbed.js"
    }
  },
  "dependencies": {
    "stage-js": "github:shakiba/stage.js#v1-alpha"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-replace": "^5.0.2",
    "@rollup/plugin-terser": "^0.4.3",
    "@types/chai": "^4.3.5",
    "@types/estree": "^0.0.50",
    "@types/mocha": "^10.0.1",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "ajv": "^8.12.0",
    "chai": "^4.3.7",
    "dtslint": "^3.6.9",
    "eslint": "^8.42.0",
    "esm": "^3.2.25",
    "express": "^4.18.2",
    "handlebars": "^4.7.6",
    "is-core-module": "^2.12.1",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "rollup": "^3.25.1",
    "rollup-plugin-filesize": "^10.0.0",
    "rollup-plugin-license": "^3.0.1",
    "rollup-plugin-ts": "^3.2.0",
    "sinon": "^15.1.2",
    "ts-node": "^9.1.1",
    "tsd": "^0.18.0",
    "typedoc": "^0.24.8",
    "typedoc-plugin-markdown": "^3.15.3",
    "typescript": "^4.0.0-beta"
  },
  "scripts": {
    "lint": "eslint './src/**/*.ts'",
    "test": "mocha 'src/**/__test__/*.test.*' --require ts-node/register",
    "test:coverage": "nyc --reporter=text mocha 'src/**/__test__/*.test.*' --require ts-node/register",
    "test:coverage:html": "nyc --reporter=html mocha 'src/**/__test__/*.test.*' --require ts-node/register",
    "test:types": "tsd",
    "build": "rollup -c",
    "watch": "rollup -c --watch",
    "testbed": "node testbed/server.js",
    "typedoc": "typedoc --options typedoc.json"
  }
}
