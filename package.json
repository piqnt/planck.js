  "scripts": {
    "test": "mocha 'src/**/__test/*.js' --require esm",
    "test:coverage": "nyc --reporter=text mocha 'src/**/__test/*.js'",
    "test:coverage:html": "nyc --reporter=html mocha 'src/**/__test/*.js'",
