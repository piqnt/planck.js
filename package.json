{
  "name": "planck-v2",
  "version": "1.4.2",
  "description": "2D JavaScript/TypeScript physics engine for cross-platform HTML5 game development",
  "homepage": "https://github.com/piqnt/planck.js",
  "keywords": [
    "box2d",
    "html5",
    "javascript",
    "typescript",
    "game",
    "physics",
    "engine",
    "2d",
    "mobile"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/piqnt/planck.js.git"
  },
  "author": "Ali Shakiba",
  "contributors": [
    "Erin Catto",
    "Ali Shakiba",
    "Oliver Zell"
  ],
  "type": "module",
  "sideEffects": [
    "./dist/planck-with-testbed.*"
  ],
  "module": "dist/planck.js",
  "main": "dist/planck.js",
  "types": "dist/planck.d.ts",
  "exports": {
    ".": {
      "types": "./dist/planck.d.ts",
      "import": "./dist/planck.js",
      "default": "./dist/planck.umd.cjs"
    },
    "./with-testbed": {
      "types": "./dist/planck-with-testbed.d.ts",
      "import": "./dist/planck-with-testbed.js",
      "default": "./dist/planck-with-testbed.umd.cjs"
    },
    "./dist/*": "./dist/*"
  },
  "files": [
    "src",
    "dist"
  ],
  "engines": {
    "node": ">=24.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.8",
    "@eslint/js": "^9.39.2",
    "@types/chai": "^5.2.3",
    "@vitest/coverage-v8": "^4.0.18",
    "ajv": "^8.17.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "planck-testbed": "*",
    "polymatic": "*",
    "prettier": "^3.8.1",
    "rimraf": "^6.1.2",
    "rollup-plugin-license": "^3.6.0",
    "sinon": "^21.0.1",
    "terser": "^5.46.0",
    "tsd": "^0.33.0",
    "tslib": "^2.8.1",
    "typedoc": "^0.28.16",
    "typedoc-plugin-markdown": "^4.10.0",
    "typescript-eslint": "^8.54.0",
    "vite": "^6.4.1",
    "vite-plugin-dts-bundle-generator": "^2.1.2",
    "vite-plugin-pages": "^0.33.3",
    "vite-plugin-typescript": "^1.0.4",
    "vite-plugin-typescript-transform": "^1.3.1",
    "vitepress": "^2.0.0-alpha.16",
    "vitest": "^4.0.18"
  },
  "scripts": {
    "lint": "eslint './src/**/*.ts'",
    "pretty": "prettier --write './src/**/*.ts'",
    "lint:example": "eslint --fix './example/**/*.ts'",
    "pretty:example": "prettier --write './example/**/*.ts'",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:types": "tsd --typings=./dist/planck.d.ts",
    "preflight": "npm run lint && npm run test:types && npm run test",
    "testbed": "npm run dev",
    "dev": "vite",
    "build": "vite build",
    "watch": "vite build --watch",
    "preview": "vite preview",
    "benchmark": "git log -n 1 && vite-node ./benchmark/node",
    "changeset": "changeset",
    "bump": "changeset version",
    "release": "changeset publish",
    "typedoc": "typedoc --options typedoc.json && mv ./docs/pages/api/README.md ./docs/pages/api/index.md",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  }
}
